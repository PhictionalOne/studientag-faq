% Theme Colors
\setbeamercolor*{palette primary}{fg=black,bg=white}
\setbeamercolor*{palette secondary}{parent=palette primary,fg=karminrot}
\setbeamercolor*{palette tertiary}{fg=anthrazit,bg=white} % fg sets font for navigation

\setbeamercolor{alerted text}{fg=karminrot}
\setbeamercolor{normal text}{parent=palette primary}

\setbeamercolor{frametitle}{parent=palette primary}
\setbeamercolor{headline}{parent=palette secondary}
\setbeamercolor{titlelike}{parent=palette primary}
\setbeamercolor{title}{fg=black,bg=white}
\setbeamercolor{title page}{fg=anthrazit}
\setbeamercolor{title on title page}{fg=karminrot}
\setbeamercolor{subtitle on title page}{fg=anthrazit}
\setbeamercolor{structure}{fg=anthrazit}

\setbeamercolor{block title}{fg=white,bg=gold}
\setbeamercolor{block body}{bg=gold!50,fg=black}

\setbeamercolor{block title alerted}{fg=white,bg=karminrot}
\setbeamercolor{block body alerted}{bg=karminrot!20,fg=black}

\setbeamercolor{block title example}{fg=white,bg=green}
\setbeamercolor{block body example}{bg=green!20,fg=black}

\setbeamercolor{item}{bg=white,fg=black}
\setbeamercolor{subitem}{bg=white,fg=black}
\setbeamercolor{subsubitem}{bg=white,fg=black}

\setbeamercolor{item projected}{fg=white,bg=tumadditionaldarkblue}
\setbeamercolor{subitem projected}{fg=white,bg=tumadditionalblue}
\setbeamercolor{subsubitem projected}{fg=white,bg=tumadditionallightblue}

\setbeamercolor{description item}{fg=black}
\setbeamercolor{description subitem}{fg=black}
\setbeamercolor{description subsubitem}{fg=black}

